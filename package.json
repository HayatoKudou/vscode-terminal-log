{
	"name": "vscode-terminal-capture",
	"displayName": "Terminal Memo",
	"description": "  Take an open terminal's output and open it in a tab to quickly navigate and edit the output.",
	"repository": {
		"type": "git",
		"url": "https://github.com/mikekwright/vscode-terminal-capture"
	},
	"version": "0.0.1",
	"publisher": "devwright",
	"engines": {
		"vscode": "^1.31.0"
	},
	"categories": [
		"Other"
	],
	"activationEvents": [
		"*"
	],
	"main": "./out/extension.js",
	"contributes": {
		"commands": [
			{
				"command": "extension.terminalMemo.runCapture",
				"title": "Terminal: Memo"
			}
		],
		"configuration": {
			"title": "Terminal Memo",
			"type": "object",
			"properties": {
				"terminalMemo.enable": {
					"type": "boolean",
					"description": "falseの場合、拡張機能の無効化",
					"default": true
				},
				"terminalMemo.useClipboard": {
					"type": "boolean",
					"description": "falseの場合、すべての履歴を追跡するキャッシュモードを使用",
					"default": true
				}
			}
		}
	},
	"scripts": {
		"vscode:prepublish": "npm run compile",
		"compile": "tsc -p ./",
		"watch": "tsc -watch -p ./",
		"postinstall": "node ./node_modules/vscode/bin/install",
		"test": "npm run compile && node ./node_modules/vscode/bin/test"
	},
	"devDependencies": {
		"typescript": "^3.3.1",
		"vscode": "^1.1.28",
		"tslint": "^5.12.1",
		"@types/node": "^10.12.21",
		"@types/mocha": "^2.2.42"
	},
	"dependencies": {
		"vsce": "^1.57.0"
	}
}